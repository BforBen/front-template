@{
    string NgApp = (string)ViewContext.ViewData["NgApp"] ?? "";
    string HtmlClass = (string)ViewContext.ViewData["HtmlClass"] ?? "";

    string AppTitle = (string)ViewContext.ViewData["ApplicationTitle"] ?? "";
    string NavBrandUrl = (string)ViewContext.ViewData["NavBrandUrl"] ?? Url.Content("~/");
    string NavBrandTitle = (string)ViewContext.ViewData["NavBrandTitle"] ?? "Link to home";
}
<!DOCTYPE html>
<html lang="en" ng-app="@NgApp" class="@HtmlClass">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Noto+Sans:400,700" rel="stylesheet">
    @Styles.Render("~/assets/css")
    <title>@if (!String.IsNullOrWhiteSpace(ViewBag.Title))
    { <text>@ViewBag.Title - </text> }@AppTitle</title>
    <link rel="apple-touch-icon" sizes="57x57" href="//static.guildford.gov.uk/assets/apple-touch-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="//static.guildford.gov.uk/assets/apple-touch-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="//static.guildford.gov.uk/assets/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="//static.guildford.gov.uk/assets/apple-touch-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="//static.guildford.gov.uk/assets/apple-touch-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="//static.guildford.gov.uk/assets/apple-touch-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="//static.guildford.gov.uk/assets/apple-touch-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="//static.guildford.gov.uk/assets/apple-touch-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="//static.guildford.gov.uk/assets/apple-touch-icon-180x180.png">
    <link rel="icon" type="image/png" href="//static.guildford.gov.uk/assets/favicon-32x32.png" sizes="32x32">
    <link rel="icon" type="image/png" href="//static.guildford.gov.uk/assets/favicon-194x194.png" sizes="194x194">
    <link rel="icon" type="image/png" href="//static.guildford.gov.uk/assets/favicon-96x96.png" sizes="96x96">
    <link rel="icon" type="image/png" href="//static.guildford.gov.uk/assets/android-chrome-192x192.png" sizes="192x192">
    <link rel="icon" type="image/png" href="//static.guildford.gov.uk/assets/favicon-16x16.png" sizes="16x16">
    <link rel="manifest" href="//static.guildford.gov.uk/assets/manifest.json">
    <link rel="shortcut icon" href="//static.guildford.gov.uk/assets/favicon.ico">
    <meta name="msapplication-TileColor" content="#00aba9">
    <meta name="msapplication-TileImage" content="//static.guildford.gov.uk/assets/mstile-144x144.png">
    <meta name="msapplication-config" content="//static.guildford.gov.uk/assets/browserconfig.xml">
    <meta name="theme-color" content="#009688">
    <script>
        var BaseUrl = '@Url.Content("~/")';
    </script>
    @RenderSection("HeadScripts", required: false)
</head>
<body id="top">
    <a class="sr-only sr-only-focusable" href="#content">Skip to main content</a>
    <header role="banner" class="navbar navbar-gbc">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="@NavBrandUrl" title="@NavBrandTitle" class="navbar-brand">
                    @AppTitle
                </a>
            </div>
            <div class="collapse navbar-collapse">
                @RenderSection("BeforePrimaryNavigation", required: false)
                @if (IsSectionDefined("PrimaryNavigation"))
                {
                    <ul class="nav navbar-nav navbar-right">
                        @RenderSection("PrimaryNavigation", required: false)
                    </ul>
                }
            </div>
        </div>
    </header>

    @RenderSection("ServiceStage", required: false)

    <div class="container">
        @if (IsSectionDefined("Breadcrumb"))
        {
            <div class="row">
                <div class="col-xs-12">
                    <ol role="breadcrumbs" class="breadcrumb">
                        @RenderSection("Breadcrumb", required: false)
                    </ol>
                </div>
            </div>
        }
        <div class="article">
            <div class="row">
                <div class="col-xs-12">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    @if (IsSectionDefined("Footer"))
    {
        <footer role="contentinfo">
            <div class="container">
            @if (IsSectionDefined("Footer"))
            {
            <div class="row">
                @RenderSection("Footer", required: false)
            </div>
            }
            <div class="row footer-appInfo">
                <div class="col-md-7">
                    <ul class="list-inline">
                        @if (!String.IsNullOrWhiteSpace((string)ViewContext.ViewData["SupportUrl"]))
                        {
                            <li><a href="@ViewContext.ViewData["SupportUrl"]" class="btn btn-link">Support</a></li> }@if (!String.IsNullOrWhiteSpace((string)ViewContext.ViewData["FeedbackUrl"]))
                            {
                                <li><a href="@ViewContext.ViewData["FeedbackUrl"]" class="btn btn-link">Feedback</a></li> }
                            <li>Built by <a href="http://guildfordbc.github.io" rel="nofollow">Business Transformation Team</a></li>
                        </ul>
                    </div>
                    <div class="col-md-5">@ViewContext.ViewData["ApplicationName"] @ViewContext.ViewData["ApplicationVersion"]</div>
                </div>

            </div>
        </footer>
    }
    <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
    @RenderSection("FooterScripts", required: false)
</body>
</html>